{
    "projectId": "zapway",
    "endpoint": "https://fra.cloud.appwrite.io/v1",
    "projectName": "zapway",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "sites": true,
            "functions": true,
            "graphql": true,
            "messaging": true

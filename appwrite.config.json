    "functions": [
        {
            "$id": "Zap",
            "execute": [
                "user:Owner"
            ],
            "name": "ZAPWAY",
            "enabled": true,
            "logging": true,
            "runtime": "node-22",
            "scopes": [],
            "events": [],
            "schedule": "",
